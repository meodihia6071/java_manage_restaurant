<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.TableTypeEntity" table="table_type" schema="restaurant_manager">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="comment">
            <column name="comment" sql-type="varchar(500)" length="500"/>
        </property>
        <property name="flag">
            <column name="flag" sql-type="int"/>
        </property>
    </class>
</hibernate-mapping>